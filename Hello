using System;

namespace ConsoleCreatures
{
  class Program
  {
    static void Main()
    {
      //This program is going to be used to create and print some line art using the keyboard
      Console.WriteLine(" .-.");
      Console.WriteLine("(o o)");
      Console.WriteLine("| O |");
      Console.WriteLine("|   |");
      Console.WriteLine("'~~~'"); 
      Console.WriteLine("Spiro the Specter"); 
     }
  }
}
